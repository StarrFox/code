# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  PremakeTesting_config = debug
endif
ifeq ($(config),release)
  PremakeTesting_config = release
endif

PROJECTS := PremakeTesting

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

PremakeTesting:
ifneq (,$(PremakeTesting_config))
	@echo "==== Building PremakeTesting ($(PremakeTesting_config)) ===="
	@${MAKE} --no-print-directory -C . -f PremakeTesting.make config=$(PremakeTesting_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f PremakeTesting.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   PremakeTesting"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"